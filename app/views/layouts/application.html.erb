  </head>

  <body>
  <h1> Organizador </h1>
<!-- Muestra los mensajes redirigidos por otra pÃ¡gina si los hay -->
<% flash.each do |key, value| %>
   <p> <%= value %> </p>
<% end %>
  

    <%= yield %>
